{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": false,
  "singleQuote": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^~/features/(.*)$",
    "^~/components/(.*)$",
    "^~/utils/(.*)$",
    "^~/api/(.*)$",
    "^~/generated/(.*)$",
    "^~/typescript/(.*)$",
    "^[./]"
  ]
}
