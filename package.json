{
  "name": "agua-vibora-generator",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "mkdir -p netlify/functions && cp -r dist/* netlify/functions/",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "date-fns": "^4.1.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ics": "^3.8.1",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pdfkit": "^0.17.2",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/pdfkit": "^0.17.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
